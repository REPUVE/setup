# ~/.ssh/config

Host github
	HostName github.com
	User git
	IdentityFile ~/.ssh/github

Host vps
	HostName zgrep.org
	User zgrep
	IdentityFile ~/.ssh/vps

Host zvehk
	HostName zgrep.org
	User vehk
	IdentityFile ~/.ssh/vps

Host vehk
    HostName vehk.de
    User zgrep
    IdentityFile ~/.ssh/vehk

Host gibson
    HostName 192.168.1.200
    User root
    IdentityFile ~/.ssh/gibson
